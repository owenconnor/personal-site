var message = "<%= j @message %>"
if (message == 'error'){
    $('#contact-form').html("<div class='form-response'><h3>Opps!</h3> There has been an error please try again later</div>");
}else {
    $('#contact-form').html("<div class='form-response'><h3>Thanks for you message,</h3> I'll get back to you really soon</div>");
}
